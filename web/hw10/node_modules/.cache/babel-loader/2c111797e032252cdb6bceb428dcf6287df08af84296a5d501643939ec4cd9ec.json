{"ast":null,"code":"export default {\n  name: \"Register\",\n  data: function () {\n    return {\n      name: \"\",\n      login: \"\",\n      password: \"\",\n      error: \"\"\n    };\n  },\n  methods: {\n    onRegister: function () {\n      this.error = \"\";\n      alert(this.name);\n      this.$root.$emit(\"onRegister\", this.name, this.login, this.password);\n    }\n  },\n  beforeMount() {\n    this.$root.$on(\"onRegisterValidationError\", error => this.error = error);\n  }\n};","map":{"version":3,"mappings":"AAuCA;EACAA;EACAC;IACA;MACAD;MACAE;MACAC;MACAC;IACA;EACA;EACAC;IACAC;MACA;MACAC;MACA;IACA;EACA;EACAC;IACA;EACA;AACA","names":["name","data","login","password","error","methods","onRegister","alert","beforeMount"],"sourceRoot":"src/components/page","sources":["Register.vue"],"sourcesContent":["<template>\n    <div class=\"register form-box\">\n        <div class=\"header\">Register</div>\n        <div class=\"body\">\n            <form @submit.prevent=\"onRegister\">\n                <div class=\"field\">\n                    <div class=\"name\">\n                        <label for=\"name\">Name</label>\n                    </div>\n                    <div class=\"value\">\n                        <input autofocus id=\"name\" name=\"name\" v-model=\"name\"/>\n                    </div>\n                </div>\n                <div class=\"field\">\n                    <div class=\"name\">\n                        <label for=\"login\">Login</label>\n                    </div>\n                    <div class=\"value\">\n                        <input autofocus id=\"login\" name=\"login\" v-model=\"login\"/>\n                    </div>\n                </div>\n                <div class=\"field\">\n                    <div class=\"name\">\n                        <label for=\"password\">Password</label>\n                    </div>\n                    <div class=\"value\">\n                        <input id=\"password\" name=\"password\" type=\"password\" v-model=\"password\"/>\n                    </div>\n                </div>\n                <div class=\"field error\">{{ error }}</div>\n                <div class=\"button-field\">\n                    <input type=\"submit\" value=\"Register\">\n                </div>\n            </form>\n        </div>\n    </div>\n</template>\n\n<script>\nexport default {\n    name: \"Register\",\n    data: function () {\n        return {\n            name: \"\",\n            login: \"\",\n            password: \"\",\n            error: \"\"\n        }\n    },\n    methods: {\n        onRegister: function () {\n            this.error = \"\";\n            alert(this.name);\n            this.$root.$emit(\"onRegister\", this.name, this.login, this.password);\n        }\n    },\n    beforeMount() {\n        this.$root.$on(\"onRegisterValidationError\", (error) => this.error = error);\n    }\n}\n</script>\n\n<style scoped>\n\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}